{
  "name": "quasar-nuxt",
  "version": "0.16.0-alpha.2",
  "description": "Nuxt Module for Quasar Framework",
  "license": "MIT",
  "contributors": [
    {
      "name": "Razvan Stoenescu <razvan.stoenescu@gmail.com>"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/quasarframework/quasar-nuxt",
  "publishConfig": {
